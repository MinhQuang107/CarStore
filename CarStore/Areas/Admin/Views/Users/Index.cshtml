@using Microsoft.AspNetCore.Identity
@*@model IEnumerable<IdentityUser>*@
@model IEnumerable<Managers>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Users";
}
<h1> App Roles </h1>
<p>
    <a asp-area="Admin" asp-controller="Roles" asp-action="Create">Create new Users</a>
</p>        
@*<a href='@Url.Action("Create", "Roles", new { area = "Admin" })'>Create</a>*@

<table class="table table-bordered">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.ID)</th>
            <th>@Html.DisplayNameFor(model => model.Name)</th>
            <th>@Html.DisplayNameFor(model => model.Phone)</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null) {
            @foreach (var item in Model)
            {
                <tr>
                    <th>@Html.DisplayFor(modelitem => item.ID)</th>
                    <th>@Html.DisplayFor(modelitem => item.Name)</th>
                    <th>@Html.DisplayFor(modelitem => item.Phone)</th>
                </tr>
            }
        }

    </tbody>
</table>